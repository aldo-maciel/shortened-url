{
    "name": "shortened-server",
    "version": "0.1.0",
    "private": true,
    "author": "Aldo Bernardes Maciel",
    "scripts": {
        "build": "tsc && tscpaths -p tsconfig.json -s ./src",
        "dev": "ts-node -r tsconfig-paths/register src/server.ts",
        "start": "NODE_ENV=production node ./dist/server.js --open",
        "prod": "npm run build && npm run start",
        "test": "NODE_ENV=test PORT=3052 mocha",
        "coverage": "nyc npm test"
    },
    "keywords": [
        "mongoose",
        "inmemory",
        "test",
        "mocha",
        "mongodb",
        "nodejs",
        "typescript",
        "chai"
    ],
    "license": "MIT",
    "dependencies": {
        "cors": "2.8.5",
        "express": "4.17.1",
        "http-status-codes": "1.4.0",
        "lodash": "^4.17.19",
        "log4js": "6.4.0",
        "mongoose": "5.9.23"
    },
    "devDependencies": {
        "@types/chai": "4.2.11",
        "@types/chai-http": "4.2.0",
        "@types/cors": "2.8.6",
        "@types/express": "4.17.7",
        "@types/lodash": "4.14.157",
        "@types/mocha": "8.0.0",
        "@types/mongodb": "3.5.25",
        "@types/mongoose": "5.7.31",
        "chai": "4.2.0",
        "chai-http": "4.3.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.4",
        "mocha": "8.0.1",
        "mongodb-memory-server": "6.6.1",
        "nodemon": "2.0.4",
        "nyc": "15.1.0",
        "prettier": "2.0.5",
        "ts-mocha": "7.0.0",
        "ts-node": "8.10.2",
        "tscpaths": "0.0.9",
        "typescript": "3.9.6"
    }
}
